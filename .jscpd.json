{
  "gitignore": true,
  "ignore": [
    "**/.git/**",
    "**/.github/**",
    "**/.jscpd-report/**",
    "**/node_modules/**",
    "**/.vscode/**",
    "**/*.spec.ts"
  ],
  "min-lines": 1,
  "min-tokens": 5,
  "mode": "strict",
  "output": ".jscpd-report",
  "reporters": ["console", "html"],
  "threshold": 0.05
}
