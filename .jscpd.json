{
  "format": ["scss", "typescript", "gherkin"],
  "gitignore": true,
  "ignore": [
    "**/.git/**",
    "**/.github/**",
    "**/.jscpd-report/**",
    "**/.vscode/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/*.spec.ts"
  ],
  "min-lines": 1,
  "min-tokens": 5,
  "mode": "strict",
  "output": ".jscpd-report",
  "reporters": ["console", "html", "time"],
  "threshold": 5
}
